{% extends 'DecathlonDevOpsBundle::layout.html.twig' %}
{% block header %}{{ parent() }} :: {{ miembro.nombre }}{% endblock %}
{% block content %}
    <h1>{{ miembro.nombre }} {{ miembro.apellido }}</h1>
    <div class="row">
        <div class="col-md-10">
            {{ miembro.bio }}
        </div>
    </div>
    <h2>Proyectos en los que participa:</h2>
    <ul>
    {% for proyecto in miembro.proyectos %}
        <li><a href="{{ path('proyecto_get', {'id' : proyecto.id}) }}">{{ proyecto.nombre }}</a></li>
    {% endfor %}
    </ul>
{% endblock %}